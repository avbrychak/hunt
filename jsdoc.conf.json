{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include" : [
      "README.md",
      "index.js",
      "lib/api/",
      "lib/generators/",
      "lib/models/",
      "lib/passportStrategies/",
      "lib/templateEngines/"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": false
    }
  },
  "opts":{
    "destination":"./examples/views/documentation"
  }
}